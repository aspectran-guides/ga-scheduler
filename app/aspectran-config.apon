context: {
    root: /config/root-config.xml
    resources: [
        /config
        /classes
    ]
    hybridLoad: false
    autoReload: {
        reloadMode: soft
        scanIntervalSeconds: 5
        startup: false
    }
    profiles: {
    }
}
scheduler: {
    startDelaySeconds: 1
    waitOnShutdown: true
    startup: true
}
console: {
    mode: jline
    prompt: "{{BLUE}}gs-console-app{{YELLOW,bold}}>{{off}} "
    showDescription: true
}